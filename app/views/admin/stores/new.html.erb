<div>
  <p class="h4">店舗新規登録</p>
  <% if @store.errors.any? %>
  <%= @store.errors.count %>件のエラーが発生しました
    <% @store.errors.full_messages.each do |message| %>
      <%= message %>
    <% end %>
  <% end %>
  <%= form_with model: @store,url: admin_stores_path, local:true do |f| %>
  <div>
    <div>
      <p>店舗画像</p>
      <p><%= f.attachment_field :images_files, multiple: true  %></p>
    </div>
    <div>
      <p>店舗地域</p>
      <p></p>
    </div>
    <div>
      <p>店舗名</p>
      <p><%= f.text_field :name %></p><br>
    </div>
    <div>
      <p>店舗説明</p>
      <p><%= f.text_area :explanation %></p><br>
    </div>
    <div>
      <%= f.submit '新規投稿' , class: "btn btn-success" %>
    </div>
    <% end %>
  </div>
</div>