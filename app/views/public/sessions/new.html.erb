 <div class="container">
  <div class="row">
    <div class="col-md-6 offset-md-3 mt-5">
      <div class="h-25">
        <span class="h2 text-left">ログイン</><span>
      </div>
      <%= form_with model: @user, url: user_session_path, local:true do |f| %>
      <div class="table-responsive-sm">
        <table class="table">
          <tbody class="table table-borderless">
            <tr>
              <td>メールアドレス </td>
              <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
            </tr>
            <tr>
              <td>パスワード</td>
              <td> <%= f.password_field :password, autocomplete: "current-password" %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="actions text-center">
        <%= f.submit "ログイン" , class: 'btn btn-success'  %>
      </div>
      <% end %>
    </div>
  </div>
</div>